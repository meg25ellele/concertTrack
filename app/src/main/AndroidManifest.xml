<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.concerttrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_channel"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_channel"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.ForgotPasswordActivity"></activity>
        <activity android:name=".ui.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.FanMainPageActivity" />
        <activity android:name=".ui.ArtistRegisterActivity" />
        <activity android:name=".ui.FanRegisterActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.MainPageActivity"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>